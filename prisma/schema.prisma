generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Configuration {
  id              String  @id @default(uuid())
  width           Int
  height          Int
  croppedImageUrl String?
  imageUrl        String?
}

model Colors {
  id    String @id @default(uuid())
  label String
  value String @unique
  tw    String
}

model ModelsPhone {
  id    String @id @default(uuid())
  label String
  value String @unique
}
